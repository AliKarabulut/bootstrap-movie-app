{
  "name": "bootstrap-movie-app",
  "version": "0.1.0",
  "private": true,
  "description": "A movie app built with Next.js and Bootstrap",
  "author": {
    "name": "Ali Karabulut",
    "email": "a.kbt6666@gmail.com",
    "url": "https://github.com/AliKarabulut"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint ./src --fix",
    "prepare": "husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.tsx": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "autoprefixer": "^10.4.18",
    "bootstrap": "^5.3.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^9.0.11",
    "next": "14.1.3",
    "prettier": "^3.1.1",
    "react": "^18",
    "react-bootstrap": "^2.10.1",
    "react-dom": "^18",
    "sass": "^1.71.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.0.4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "typescript": "^5"
  }
}
